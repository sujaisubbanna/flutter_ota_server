{
  "name": "flutter-ota-server",
  "version": "0.0.1",
  "description": "Flutter OTA Server",
  "scripts": {
    "watch": "tsc --watch --preserveWatchOutput --pretty",
    "dev": "cross-env NODE_ENV=development nodemon",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "rimraf dist && tsc",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "diff-match-patch-typescript": "^1.0.8",
    "dotenv": "^10.0.0",
    "fastify": "^3.19.2",
    "fastify-env": "^2.1.1",
    "rmfr": "^2.0.0",
    "simple-git": "^2.41.2"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/node": "latest",
    "@types/rmfr": "^2.0.1",
    "fastify-tsconfig": "^1.0.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
